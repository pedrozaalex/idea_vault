{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build && yarn build:tailwind",
    "build:tailwind": "tailwindcss --minify --iinput ./styles/app.css --output app/styles/tailwind.css",
    "dev": "cross-env NODE_ENV=development remix dev && yarn dev:tailwind",
    "dev:tailwind": "tailwindcss --input ./styles/app.css --output app/styles/tailwind.css --watch",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@ant-design/icons": "4.0.0",
    "@remix-run/eslint-config": "^1.2.2",
    "@remix-run/vercel": "^1.2.2",
    "@remix-run/react": "^1.2.2",
    "@tiptap/core": "^2.0.0-beta.174",
    "@tiptap/extension-typography": "^2.0.0-beta.20",
    "@tiptap/extension-underline": "^2.0.0-beta.23",
    "@tiptap/react": "^2.0.0-beta.108",
    "@tiptap/starter-kit": "^2.0.0-beta.183",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@remix-run/dev": "^1.2.2",
    "@remix-run/serve": "^1.2.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "daisyui": "^2.2.0",
    "miniflare": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
